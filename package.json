{
    "name": "updaowns-ubl",
    "version": "1.0.0",
    "description": "Unblocked games website with tab cloaking and other features",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "build": "npm run clean && npm run copy-assets",
        "clean": "rimraf dist",
        "copy-assets": "copyfiles -u 1 \"assets/**/*\" dist/",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "keywords": [
        "math games",
        "school help",
        "web"
    ],
    "author": "Updaown",
    "license": "MIT",
    "dependencies": {
        "@zxing/browser": "^0.1.5",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "qrcode.react": "^4.2.0"
    },
    "devDependencies": {
        "copyfiles": "^2.4.1",
        "eslint": "^8.56.0",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.1",
        "rimraf": "^5.0.5"
    }
}
